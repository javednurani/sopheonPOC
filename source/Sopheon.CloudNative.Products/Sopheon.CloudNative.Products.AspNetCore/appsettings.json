{
	"AzureAdB2C": {
		"Instance": "https://^B2CTenantLoginName^.b2clogin.com",
		"Domain": "nonexistent.onmicrosoft.com",
		"ClientId": "^B2CProductManagementApiClientId^",
		"TenantId": "^B2CTenantId^",
		"SignedOutCallbackPath ": "/signout-callback-oidc",
		"SignUpSignInPolicyId": "B2C_1A_SIGNUP_SIGNIN",
		"ClientSecret": "[Not Needed Using Auth Code + PKCE]",
		"AllowWebApiToBeAuthorizedByACL": true,
		"OwnerIdClaim": "http://schemas.xmlsoap.org/ws/2005/05/identity/claims/nameidentifier"
	},
	"Swagger": {
		"ClientId": "^B2CProductManagementApiSwaggerClientId^"
	},
	"ServiceUrls": {
		"EnvironmentsBaseUrl": "https://^EnvironmentsFunctionAppName^.azurewebsites.net",
		"GetEnvironmentResourceBindingUri": "/GetEnvironmentResourceBindingUri",
		"GetEnvironments": "/environments"
	},
	"Logging": {
		"LogLevel": {
			"Default": "Debug",
			"Microsoft": "Warning",
			"Microsoft.Hosting.Lifetime": "Information"
		}
	},
	"AllowedHosts": "*"
}
