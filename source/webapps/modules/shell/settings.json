{
  "appTitle": "Main Shell",
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true
  },
  "editor.detectIndentation": false,
  "editor.formatOnSave": true,
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "baseUrl": "/",
  "debugMode": false,
  "logLevel": "warn",
  "maxSizeDataUrl": 1,
  "stats": true,
  "testMode": false,
  "port": 3000,
  "folderPath": "/"
}
